<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efeef3"
    tools:context=".CalendarActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarr"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tttitttle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="День приема"
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarr"
        app:layout_constraintEnd_toEndOf="@+id/toolbarr"
        app:layout_constraintStart_toStartOf="@+id/toolbarr"
        app:layout_constraintTop_toTopOf="@+id/toolbarr" />

    <ImageView
        android:id="@+id/finish"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:rotation="180"
        android:src="@drawable/arrow_right_icon"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbarr" />

    <TextView
